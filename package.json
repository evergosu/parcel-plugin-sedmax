{
  "name": "sedmax-dev-kit",
  "version": "1.0.0",
  "description": "zero configuration kit for remote development",
  "main": "src/index.js",
  "scripts": {
    "start": "npx parcel watch src/index.ts -o serve.js --target node --no-source-maps",
    "build": "npx parcel build src/index.ts -o serve.js --target node --no-source-maps",
    "ts": "tsc --noEmit",
    "ts:watch": "npm run ts -- --watch",
    "lint": "eslint ./src/**/*.{ts,tsx}",
    "lint:fix": "eslint --debug ./src/**/*.{ts,tsx} --fix"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:evergosu/parcel-sedmax-dev-server.git"
  },
  "bin": {
    "sedmax": "bin/cli.js"
  },
  "keywords": [
    "sedmax",
    "parcel",
    "development"
  ],
  "author": "evergosu <evergosu@gmail.com> (https://evergosu.dev)",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.6.0",
    "@babel/core": "^7.6.0",
    "@babel/preset-env": "^7.6.0",
    "@babel/preset-typescript": "^7.6.0",
    "@types/cheerio": "^0.22.13",
    "@types/compression": "^1.0.1",
    "@types/express": "^4.17.1",
    "@types/fs-extra": "^8.0.0",
    "@types/http-proxy-middleware": "^0.19.3",
    "@types/node": "^12.7.5",
    "@types/parcel-bundler": "^1.12.1",
    "@types/request": "^2.48.3",
    "@types/request-promise": "^4.1.44",
    "@types/yargs": "^13.0.2",
    "@typescript-eslint/eslint-plugin": "^2.2.0",
    "@typescript-eslint/parser": "^2.2.0",
    "boxen": "^4.1.0",
    "eslint": "^6.3.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "^6.3.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "parcel-bundler": "^1.12.3",
    "prettier": "^1.18.2",
    "typescript": "^3.6.3"
  },
  "dependencies": {
    "chalk": "^2.4.2",
    "cheerio": "^1.0.0-rc.3",
    "compression": "^1.7.4",
    "express": "^4.17.1",
    "fs-extra": "^8.1.0",
    "http-proxy-middleware": "^0.20.0",
    "request": "^2.88.0",
    "request-promise": "^4.2.4",
    "yargs": "^14.1.0"
  }
}